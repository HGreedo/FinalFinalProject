{
  "name": "oregon-fcb",
  "version": "1.0.0",
  "description": "an encrypted website where small brands are able to connect with boutiques",
  "main": "server.js",
  "engines": {
    "npm": "6.x"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/HGreedo/FinalFinalProject.git"
  },
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "mongodb": "^3.6.9",
    "mongoose": "^5.12.13",
    "react-dom": "^16.13.1",
    "react-scripts": "3.4.3",
    "if-env": "^1.0.4"
  },
  "keywords": [
    "node.js",
    "express",
    "rest",
    "api",
    "mongodb",
    "react-dom",
    "react-scripts"
  ],
  "devDependencies": {
    "concurrently": "^4.1.0",
    "nodemon": "^1.18.7"
  },
  "scripts": {
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node scripts/seedDB.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "npm run build"
  },
  "author": "Harrison Reed",
  "license": "ISC"
}
